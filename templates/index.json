      "settings": {
        "subheading": "",
        "title": "",
        "html": "<div style=\"text-align: center; color: white; background-color:#204A80; padding-bottom: 20px;\">\n<img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0641\/9093\/9354\/files\/Quality_Care_Suite_logo-removebg-preview_720x.png?v=1652379824\">\n<p style=\"width: 70%; text-align: center; display: inline-flex;\">We function as the sole IT department for over 100 Small Businesses in Middle Georgia. Whether you are looking to outsource your IT department or looking for a new hardware solution, or even need to borrow an extra head to troubleshoot a larger issue, we have a solution for you.<\/p>\n<p>\nComputer Security<br>\nInternet Security<br>\nComputer Assistance<br>\nNetwork Assistance<br>\nAntivirus Assistance<br>\nMaintenance &amp; Monitoring<br>\nRemote Support<br>\n<\/p>\n<a href=\"\/pages\/contact\" class=\"button button\" style=\"color:#204A80; background-color: white; line-height: inherit; \">Schedule an Appointment <br>Or <br>Request a Quote<\/a>\n<\/div>",
        "add_vertical_spacing": true,
